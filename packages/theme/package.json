{
  "name": "@christian-slater/theme",
  "version": "2.0.1",
  "scripts": {
    "start": "christian-slater watch",
    "build": "christian-slater build",
    "deploy:development": "christian-slater build && christian-slater sync",
    "deploy:production": "christian-slater build && christian-slater sync --theme production",
    "test:start": "christian-slater watch --config test.config.js",
    "test:build": "christian-slater build --config test.config.js",
    "test:deploy": "christian-slater build --config test.config.js && christian-slater sync --config test.config.js",
    "test:deploy:production": "christian-slater build --config test.config.js && christian-slater sync --theme production --config test.config.js"
  },
  "author": "https://thecouch.nyc",
  "license": "MIT",
  "homepage": "https://github.com/the-couch/christian-slater#readme",
  "devDependencies": {
    "christian-slater": "^2.0.1"
  },
  "dependencies": {
    "choozy": "0.0.3",
    "lazim": "^1.0.0",
    "mitt": "^1.1.3",
    "operator": "^1.4.0",
    "picoapp": "^3.1.3",
    "sliced": "^1.0.1",
    "svbstrate": "^4.1.1",
    "unfetch": "^4.1.0"
  },
  "gitHead": "c19d8b19fef77ad7e1e693b28eb0d159b608ba70"
}
